    "def are_rotations(str1, str2):\n",
    "    if len(str1) != len(str2):\n",
    "        return False\n",
    "\n",
    "    concatenated_str = str1 + str1\n",
    "\n",
    "    return str2 in concatenated_str\n",
    "\n",
    "string1 = \"hello\"\n",
    "string2 = \"lohel\"\n",
    "result = are_rotations(string1, string2)\n",
    "print(result)"
