   "source": [
    "def reverse_stack(stack):\n",
    "    aux_stack = []\n",
    "\n",
    "    while stack:\n",
    "        aux_stack.append(stack.pop())\n",
    "\n",
    "    while aux_stack:\n",
    "        stack.append(aux_stack.pop())\n",
    "\n",
    "my_stack = [1, 2, 3, 4, 5]\n",
    "reverse_stack(my_stack)\n",
    "print(f\"Reversed Stack: {my_stack}\")"
   ]
  }
